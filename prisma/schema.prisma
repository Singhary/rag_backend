generator db {
  provider             = "prisma-client-py"
  interface            = "asyncio"
  recursive_type_depth = 5
}

datasource db {
  provider = "postgresql"
  url   = env("DATABASE_URL")
}

model Application_logs {
    id Int @id @default(autoincrement())
    session_id String
    user_query String
    gpt_response String
    model String
    created_at DateTime @default(now())
}

model Document_store {
    id Int @id @default(autoincrement())
    filename String
    upload_timestamp DateTime @default(now())
}
